{{> sideNav }}
<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
  <div class="pcoded-content">
    <!-- [ Main Content ] start -->
    <div class="p-0">
      <div class="p-0 contant-holder">
        <!-- contant holder start -->
        <h3 class="main-head-3">Campaign Report</h3>
        <!-- contant holder end -->
        <div class="col-md-12 pt-2 pb-5">
          <div class="frm-border">
            <form id="controls-form">
              <div class="row">
                <div class="col-md-2">
                  <div class="form-group">
                    <label for="txt-from">From</label>
                    <input type="text" class="form-control" id="fromDate" required>
                  </div>
                </div>
                <div class="col-md-2">
                  <div class="form-group">
                    <label for="txt-from">To</label>
                    <input type="text" class="form-control" id="toDate" required>
                  </div>
                </div>
                <div class="col-md-2">
                  <div class="form-group mt-4 pt-1 d-flex justify-content-center">
                    <input type="hidden" id="pageNumber" value="1" />
                    <input type="hidden" id="totalPages" value="1" />
                    <button type="submit" class="btn btn-primary">Submit</button>
                  </div>
                </div>
                <div class="col-md-2 form-group mt-4 pt-1 ">
                  <button class="btn btn-warning d-none" id="donwloadCampaignReport"></button>
                </div>
              </div>
            </form>
          </div>

          <table id="misTable" class="table table-striped table-sm table-bordered display responsive no-wrap"
            style="width:100%">
            <thead>
              <tr>
                <th class="all">Summary Date</th>
                <th class="all">Campaign Name</th>
                <th class="all">Campaign Id</th>
                <th class="all">Total Request</th>
                <th class="all">Total Rejected</th>
                <th class="all">Total Submit</th>
                <th class="none">Total Delivered</th>
                <th class="none">Total Failed</th>
                <th class="none">Total Awaited</th>
                <th class="none">Total Clicks</th>
                <th class="none">Sender ID</th>
                <th class="all">Action</th>
              </tr>
            </thead>
          </table>
          <div class="tablePageNavi text-center">
            <ul id="misPagination" class="pagination table-pagination">
              <li class="page-item"><a class="page-link" id="pagePrev" href="#">Previous</a></li>
              <li class="page-item"><a class="page-link" id="pageNext" href="#">Next</a></li>
            </ul>
          </div>
          {{> mainLoader }}

        </div>
      </div>
    </div>
  </div>
  <div class="footer-div">
    <span class="copy float-left">
      <p id="demo"></p> &copy; All Rights Reserved
    </span>
    <span class="float-right">
      <ul class="social-media-icon">
        <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-rss" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-whatsapp" aria-hidden="true"></i></a></li>
      </ul>
    </span>
  </div>
</div>
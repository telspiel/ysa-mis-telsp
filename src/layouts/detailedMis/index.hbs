<!-- [ Main Content ] start -->
<div class="pcoded-main-container" style="background-color:#f6f7fb;">
  <div class="pcoded-content">
    <!-- [ Main Content ] start -->
    <div class="p-0">
      <div class="p-0 contant-holder">
        <!-- contant holder start -->
        <h3 class="main-head-3">Detailed Report</h3>
        <!-- contant holder end -->
        <div class="col-md-12 pt-2 pb-5">

          <div class="frm-border">
            <form id="controls-form">
              <div class="row">
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="fromDate">From</label>
                    <input type="text" class="form-control datepicker" id="fromDate">
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="toDate">To</label>
                    <input type="text" class="form-control datepicker" id="toDate">
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="mobileNumber">Mobile Number</label>
                    <input type="text" id="mobileNumber" class="form-control" placeholder="Mobile Number"
                      id="mobileNumber" aria-label="Mobile Number" aria-describedby="mobile-addon" >
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="txt-from">Sender ID</label>
                    <input type="text" class="form-control" id="senderid" placeholder="Sender Id"
                      {{!-- aria-label="Sender Id" pattern="^[a-zA-Z]{6}|[0-9]{6}$" maxlength="6" > --}}
                      aria-label="Sender Id" minlength="3" maxlength="14" >
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="txt-from">Message ID</label>
                    <input type="text" class="form-control" id="messageId" placeholder="Message Id"
                      aria-label="Message ID" >
                  </div>
                </div>
                <div class="col-md-2">
                  <div class="form-group mt-4 pt-2">
                    <input type="hidden" id="pageNumber" value="1" />
                    <input type="hidden" id="totalPages" value="1" />
                      <button type="submit" class="btn btn-primary">Submit</button>
                  </div>
                </div>
                <div class="col-md-2 form-group mt-4 pt-2">
                  <button class="btn btn-info d-none" id="donwloadFileButton"></button>
                </div>
              </div>
              <input type="hidden" id="pageNumber" value="1" />
              <input type="hidden" id="totalPages" value="1" />
            </form>
          </div>

          <table id="misDetailedReport" class="table table-striped table-sm table-bordered dt-responsive nowrap"
            style="width:100%">

          </table>

          <div class="paginationNav float-right" style="margin-top:10px;">
            <ul id="misPagination" class="pagination table-pagination">
              <li class="page-item"><a class="page-link" id="pagePrev" href="#">Previous</a></li>
              <li class="page-item"><a class="page-link" id="pageNext" href="#">Next</a></li>
            </ul>
          </div>

        </div>

      </div>
    </div>
  </div>
  <div class="footer-div">
    <span class="copy float-left">
      <p id="demo"></p> &copy; All Rights Reserved
    </span>
    <span class="float-right">
      <ul class="social-media-icon">
        <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-rss" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-whatsapp" aria-hidden="true"></i></a></li>
      </ul>
    </span>
  </div>
</div>
<!-- [ Main Content ] end -->
